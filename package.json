{
  "name": "marketing-machine",
  "version": "1.0.0",
  "description": "Automated LinkedIn content generation from meeting transcripts",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd frontend && npm install",
    "install:all": "npm run install:backend && npm run install:frontend",
    "build": "cd frontend && npm run build",
    "migrate": "cd backend && npm run migrate",
    "generate": "cd backend && npm run generate",
    "setup": "npm run install:all && npm run migrate && npm run generate"
  },
  "keywords": ["marketing", "linkedin", "automation", "ai"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}